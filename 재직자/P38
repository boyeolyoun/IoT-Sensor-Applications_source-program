import RPi.GPIO as GPIO
import time

LED = 17

GPIO.setmode(GPIO.BCM)
GPIO.setup(LED, GPIO.OUT)

try :
    while True :
        GPIO.output(LED, False)
        print("LED Off ...")
        time.sleep(0.5)

        GPIO.output(LED, True)
        print("LED On ...")
        time.sleep(0.5)
except :
    GPIO.cleanup()
    print("end")
